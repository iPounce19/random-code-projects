@using Sandbox;
@using Sandbox.UI;
@inherits PanelComponent
@namespace Sandbox

<root>
	<div class="Name">@itemName</div>
</root>

@code
{
	TestItems item;

	string itemName;

	protected override void OnStart(){
		item = GameObject.GetComponentInParent<TestItems>();
	}
	protected override int BuildHash()
	{
		
		itemName = item.itemName;
		this.GameObject.SetParent(null, false);
		return System.HashCode.Combine(itemName);
	}
}
